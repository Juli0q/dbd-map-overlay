{
  "name": "dbd-map",
  "version": "1.2.5",
  "description": "Overlay for DBD maps",
  "main": "index.js",
  "author": "Luca <admin@lucaservers.com>",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder -wl --publish always",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "license": "ISC",
  "build": {
    "productName": "Dbd Map Overlay",
    "appId": "com.github.lucafontanot.dbdmapoverlay",
    "linux": {
      "target": [
        "deb",
        "rpm",
        "AppImage"
      ],
      "category": "Games"
    },
    "win": {
      "target": [
        "nsis",
        "portable"
      ]
    },
    "files": [
      "!static/*",
      "!scripts/*",
      "!dist/*",
      "!maps/*",
      "!.github/*"
    ]
  },
  "devDependencies": {
    "electron": "^35.0.3",
    "electron-builder": "^25.1.8"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "electron-updater": "^6.3.9",
    "axios": "^1.8.0",
    "bootstrap": "^5.3.1",
    "cron": "^4.1.0",
    "image-size": "^1.0.2",
    "jquery": "^3.7.0",
    "marked": "^15.0.7",
    "popper.js": "^1.16.1",
    "sharp": "^0.33.5",
    "uuid": "^11.1.0"
  }
}
